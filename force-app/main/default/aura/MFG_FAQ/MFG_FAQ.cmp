<aura:component controller="MFG_FAQController">
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>  
    <aura:attribute name="helpFaq" type="List" default="[]"/> 
    <aura:iteration items="{!v.helpFaq}" var="item">   
        <c:MFG_FAQ_Item item="{!item}"/>
    </aura:iteration>
</aura:component>