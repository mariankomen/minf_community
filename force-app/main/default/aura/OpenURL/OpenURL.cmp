<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,lightning:availableForFlowScreens,force:lightningQuickAction" access = "global">
	<aura:attribute name="url" type="String"/>
    <aura:attribute name="mode" type="String" default= "_blank" />
    <aura:attribute name="tooltip" type="String" default = "none" />
    <aura:attribute name="URLLabel" type="String" default= "test" />
    
    <lightning:formattedUrl value="{!v.url}" tooltip="{!v.tooltip}" label="{!v.URLLabel}" target="{!v.mode}" />
      
    
</aura:component>