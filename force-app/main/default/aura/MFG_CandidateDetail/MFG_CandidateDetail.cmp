                                                                aura:id="statusFields"
                                                                label="{!$Label.c.MFG_Comments_Rejection}"
                                                                maxlength="10000"
                                                                messageWhenTooLong="A maximum of 10000 characters are allowed." 
                                                                messageWhenValueMissing="Comments about Rejection required." />
                                            
                                            
                                        </div>
                                    </aura:if>
                                    
                                </div>
                                <div class="jss905">
                                    
                                    <aura:if isTrue="{! if(v.candidate.Rejection_Reason_for__c!='Candidate did not attend interview',true,false)}">
