                                             style="width: 100%; display: inline-flex; font-weight: 700;">
                                            <div class="MuiFormControl-root MuiTextField-root jss1074 MuiFormControl-fullWidth">
                                                <div class="MuiInputBase-root MuiFilledInput-root MuiFilledInput-underline MuiInputBase-fullWidth MuiInputBase-formControl">
                                                  
                                                    <ui:inputSelect class="MuiSelect-root MuiSelect-select MuiSelect-filled MuiInputBase-input MuiFilledInput-input 
                                                                           jss112 MuiInputBase-inputSelect MuiFilledInput-inputSelect"
                                                                    required="true" 
                                                                    value="{!v.candidate.Candidate_Rating__c}"
                                                                    aura:id="MFG_Rating"
                                                                    >
                                                     	<ui:inputSelectOption text="" label="--"  />
                                                        <aura:iteration items="{!v.fieldOptions.ratingOptions}" var="option">
                                                            <ui:inputSelectOption text="{!option.value}"  
                                                                                  label="{!if(v.language=='fr',option.label_fr,option.label)}" 
                                                                                  value="{! if(v.candidate.Candidate_Rating__c==option.value,true,false)}"/>
                                                        </aura:iteration>
                                                    </ui:inputSelect> 
                                                    
                                                    
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    </aura:if>

                                </div>
                            </div>
                        </div>
                    </div>
                    
                    <div class="MuiDialogActions-root MuiDialogActions-spacing">
