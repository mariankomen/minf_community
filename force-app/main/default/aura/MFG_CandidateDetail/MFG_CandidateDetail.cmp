                                                                    aura:id="MFG_Status"
                                                                    >
                                                        <ui:inputSelectOption text="" label="--"  />
                                                        <aura:iteration items="{!v.fieldOptions.statusOptions}" var="option">
                                                            <ui:inputSelectOption text="{!option.value}"  
                                                                                  label="{!if(v.language=='fr',option.label_fr,option.label)}" 
                                                                                  value="{! if(v.candidate.Status__c==option.value,true,false)}"/>
                                                        </aura:iteration>
                                                    </ui:inputSelect> 
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="jss905">
                                    <aura:if isTrue="{! if(v.candidate.Status__c=='Rejected',true,false)}">
                                    <div style="flex: 1 1 0%; display: block; padding-bottom: 4px; padding-right: 8px;">
                                        <div class="MuiTypography-root MuiTypography-overline">{!$Label.c.MFG_Rejection_Reason}</div>
                                        <div class="MuiTypography-root MuiTypography-body1"
                                             style="width: 100%; display: inline-flex; font-weight: 700;">
                                            <div class="MuiFormControl-root MuiTextField-root jss1074 MuiFormControl-fullWidth">
                                                <div class="MuiInputBase-root MuiFilledInput-root MuiFilledInput-underline MuiInputBase-fullWidth MuiInputBase-formControl">
                                                    <ui:inputSelect class="MuiSelect-root MuiSelect-select MuiSelect-filled MuiInputBase-input MuiFilledInput-input 
                                                                           jss112 MuiInputBase-inputSelect MuiFilledInput-inputSelect"
                                                                    required="true" 
                                                                    value="{!v.candidate.Rejection_Reason_for__c}"
                                                                    aura:id="MFG_Reason">
                                                        <ui:inputSelectOption text="" label="--"  />
                                                        <aura:iteration items="{!v.fieldOptions.rejectionReasons}" var="option">
                                                            <ui:inputSelectOption text="{!option.value}"  
                                                                                  label="{!if(v.language=='fr',option.label_fr,option.label)}" 
                                                                                  value="{! if(v.candidate.Rejection_Reason_for__c==option.value,true,false)}"/>
                                                        </aura:iteration>
                                                    </ui:inputSelect> 
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    </aura:if>

                                </div>
                                <div class="jss905">
                                    <aura:if isTrue="{! if( or(v.candidate.Rejection_Reason_for__c=='Candidate did not present well',
