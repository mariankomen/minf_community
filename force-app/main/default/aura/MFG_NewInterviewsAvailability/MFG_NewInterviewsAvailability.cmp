        <div class="MuiPaper-root MuiPaper-elevation3 MuiPaper-rounded" style="padding: 16px;">


            <lightning:recordEditForm objectApiName="MFG_Hiring_Manager_Availability__c" aura:id="newHrAvailability"
                onload="{!c.handleLoadEdit}" onsubmit="{!c.handleSubmitEdit}" onsuccess="{!c.handleSuccessEdit}">
                <lightning:messages />

                <div class="slds-grid slds-wrap">
                    <div class="slds-col slds-size_1-of-1 slds-large-size_12-of-12 slds-p-horizontal_x-small">

                        <label class="slds-form-element__label"><span style="color:#c23934">*</span>
                            {!$Label.c.MFG_Interview_Start_Date}</label>
                        <lightning:inputField fieldName="Interview_Start_Date__c" variant="label-hidden"
                            onchange="{!c.changeValue}" aura:id="Interview_Start_Date" />
                    </div>
                    <div class="slds-col slds-size_1-of-1 slds-large-size_6-of-12 slds-p-horizontal_x-small">
                        <lightning:input type="time" name="Start Time" label="{!$Label.c.MFG_Start_Time}"
                            required="true" value="{!v.startTime}" onchange="{!c.changetimevalue}" aura:id="startTime"
                            min="07:00:00.000" max="19:00:00.000" />
                    </div>
                    <div class="slds-col slds-size_1-of-1 slds-large-size_6-of-12 slds-p-horizontal_x-small">
                        <lightning:input type="time" name="End Time" label="{!$Label.c.MFG_End_Time}" required="true"
                            value="{!v.endTime}" onchange="{!c.changetimevalue}" aura:id="endTime" min="07:00:00.000"
                            max="19:00:00.000" />
                    </div>

                    <div class="slds-col slds-size_1-of-1 slds-large-size_12-of-12 slds-p-horizontal_x-small">
                        <label class="slds-form-element__label"><span style="color:#c23934">*</span>
                            {!$Label.c.MFG_Interview_Duration}</label>
                        <lightning:inputField fieldName="Interview_Duration__c" variant="label-hidden"
                            onchange="{!c.changeValue}" aura:id="Interview_Duration" />
                    </div>


                    <div class="slds-col slds-size_1-of-1 slds-large-size_12-of-12 slds-p-horizontal_x-small">
                        <label class="slds-form-element__label"><span style="color:#c23934">*</span>
                            {!$Label.c.Repeat_for_X_weeks}</label>
                        <lightning:inputField fieldName="Repeat_for_x_Weeks__c" variant="label-hidden"
                            onchange="{!c.changeValue}" aura:id="Repeat_for_x_Weeks" />
                    </div>

                    <div class="slds-col slds-size_1-of-1 slds-large-size_12-of-12 slds-p-horizontal_x-small"
                        style="{!if(v.reOccur,'display: block;','display: none;')}">
                        <label class="slds-form-element__label"><span style="color:#c23934">*</span>
                            {!$Label.c.Recurs_Every_Week_On}</label>
                        <lightning:inputField fieldName="Recurs_every_week_on__c" variant="label-hidden"
                            onchange="{!c.changeValue}" aura:id="Recurs_every_week_on" />
                    </div>

                    <div class="slds-col slds-size_1-of-1 slds-large-size_12-of-12 slds-p-horizontal_x-small">
                        <label class="slds-form-element__label">{!$Label.c.MFG_Recurrence_Ends_On}</label>
                        <ui:outputDate aura:id="oDate" value="{!v.Recurrence_Ends_OnV}" />
                    </div>
                    <div class="slds-col slds-size_1-of-1 slds-large-size_12-of-12 slds-p-horizontal_x-small"
                        style="visibility: hidden; height: 0px;">

                        <lightning:inputField fieldName="Recurrence_Ends_On__c" variant="label-inline" disabled="true"
                            aura:id="Recurrence_Ends_On" required="false" class="asoutput"
                            value="{!v.Recurrence_Ends_OnV}" />


                    </div>

                </div>


                <div class="slds-m-top_medium" style="padding: 16px 0px;">
                    <lightning:button variant="brand" type="submit" name="save" label="{!$Label.c.MFG_SAVE}"
                        class="MuiButtonBase-root MuiButton-root MuiButton-contained MuiButton-containedPrimary"
                        onclick="{!c.validate}" />

                    <lightning:button variant="base" type="submit" name="saveAndNew"
                        label="{!$Label.c.MFG_SAVE_AND_NEW}"
                        class="MuiButtonBase-root MuiButton-root MuiButton-text MuiButton-textPrimary"
                        onclick="{!c.validate}" />

                    <lightning:button variant="base" name="save" label="{!$Label.c.MFG_CANCEL}"
                        class="MuiButtonBase-root MuiButton-root MuiButton-text MuiButton-textPrimary"
                        onclick="{!c.cancelEditMode}" />
                </div>
            </lightning:recordEditForm>


        </div>
    </div>
        <aura:if isTrue="{! !v.hasAvailability}">
